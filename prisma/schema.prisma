generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Student {
  id                  String @id @default(uuid())
  name                String
  age                 Int
  firstSemesterGrade  Float
  secondSemesterGrade Float
  teacherName         String
  roomNumber          Int
}
